{
  "expo": {
    "name": "GeoAir",
    "scheme": "geoair",
    "icon": "./src/assets/icon.png",
    "slug": "GeoAir",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.41",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bastienchoisy.geoair",
      "buildNumber": "1.0.41"
    },
    "android": {
      "package": "com.bastienchoisy.geoair",
      "versionCode": 30,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "MANAGE_DOCUMENTS",
        "READ_EXTERNAL_STORAGE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "com.android.launcher.permission.INSTALL_SHORTCUT"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
    },
    "androidNavigationBar": {
      "visible": false,
      "barStyle": "light-content",
      "backgroundColor": "#000"
    }
  }
}
